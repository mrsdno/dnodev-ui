{
  "name": "dnodev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json && yarn copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css public/**/*.css public/**/*.js build/",
    "start": "node server",
    "watch": "nodemon --ext *",
    "dev": "cross-env NODE_ENV=development yarn build && npx nodemon build/server.js",
    "test": "cross-env ELASTIC_APM_ENABLED=false NODE_ENV=test ./node_modules/.bin/jest --watchAll --logHeapUsage"
  },
  "dependencies": {
    "@georgedoescode/spline": "^1.0.1",
    "@sinclair/typebox": "^0.24.41",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.3",
    "@types/pump": "^1.1.1",
    "@types/sharp": "^0.31.0",
    "@types/ws": "^8.5.3",
    "axios": "^0.27.2",
    "connect-livereload": "^0.6.1",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "jest": "^29.3.1",
    "jest-offline": "^1.0.1",
    "livereload": "^0.9.3",
    "morgan": "~1.9.1",
    "simplex-noise": "^4.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.13.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.18.2",
    "jest-junit": "^15.0.0",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.21",
    "prettier": "^2.5.1",
    "rimraf": "^4.3.1",
    "typescript": "^4.9.5",
    "yarn": "^1.22.18"
  }
}
